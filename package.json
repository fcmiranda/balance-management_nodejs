{
  "name": "case_itau_nodejs",
  "version": "1.0.0",
  "main": "src/app.ts",
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/app.ts",
    "dev": "nodemon --exec ts-node src/app.ts",
    "seed": "ts-node src/scripts/seed-database.ts",
    "seed:typeorm": "ts-node src/scripts/seed-typeorm-database.ts",
    "setup:typeorm": "ts-node src/scripts/setup-typeorm.ts",
    "biome:check": "biome check .",
    "biome:fix": "biome check --write .",
    "biome:format": "biome format --write .",
    "prepare": "husky",
    "typeorm": "typeorm-ts-node-esm -d ormconfig.ts",
    "migration:generate": "typeorm-ts-node-esm migration:generate -d ormconfig.ts",
    "migration:create": "typeorm-ts-node-esm migration:create",
    "migration:run": "typeorm-ts-node-esm migration:run -d ormconfig.ts",
    "migration:revert": "typeorm-ts-node-esm migration:revert -d ormconfig.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.0.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "reflect-metadata": "^0.2.2",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.25",
    "zod": "^3.25.74"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.0.10",
    "@types/sqlite3": "^3.1.11",
    "husky": "^9.0.11",
    "lint-staged": "^15.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "lint-staged": {
    "**/*.{js,mjs,cjs,jsx,ts,mts,cts,tsx}": [
      "biome check --apply",
      "biome format --write"
    ]
  }
}
