{
  "name": "case_itau_nodejs",
  "version": "1.0.0",
  "main": "src/app.ts",
  "scripts": {
    "build": "tsc",
    "start": "ts-node -r tsconfig-paths/register src/app.ts",
    "seed:typeorm": "ts-node -r tsconfig-paths/register src/scripts/seed-typeorm-database.ts",
    "setup:typeorm": "ts-node -r tsconfig-paths/register src/scripts/setup-typeorm.ts",
    "biome:check": "biome check .",
    "biome:fix": "biome check --write .",
    "biome:format": "biome format --write .",
    "prepare": "husky",
    "typeorm": "typeorm-ts-node-esm -d ormconfig.ts",
    "migration:generate": "typeorm-ts-node-esm migration:generate -d ormconfig.ts",
    "migration:create": "typeorm-ts-node-esm migration:create",
    "migration:run": "typeorm-ts-node-esm migration:run -d ormconfig.ts",
    "migration:revert": "typeorm-ts-node-esm migration:revert -d ormconfig.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPatterns='tests/(domain|application|infrastructure)'",
    "test:integration": "jest --testPathPatterns='tests/integration'",
    "test:controllers": "jest --testPathPatterns='tests/controllers'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.0.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "reflect-metadata": "^0.2.2",
    "sqlite3": "^5.1.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.25",
    "zod": "^3.25.74"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.0.10",
    "@types/sqlite3": "^3.1.11",
    "@types/supertest": "^6.0.3",
    "husky": "^9.0.11",
    "jest": "^30.0.4",
    "lint-staged": "^15.5.2",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "lint-staged": {
    "**/*.{js,mjs,cjs,jsx,ts,mts,cts,tsx}": [
      "biome check --write",
      "biome format --write"
    ]
  }
}
